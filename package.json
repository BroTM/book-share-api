{
  "name": "book-share-api",
  "version": "1.0.0",
  "description": "sequelize orm(mysql), pug, typescript, nodejs",
  "main": "app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc && node --unhandled-rejections=strict ./dist/app.js",
    "start:dev": "nodemon",
    "debug": "set debug=* && npm run start",
    "build": "tsc",
    "serve": "node dist/app.js",
    "test:no-reloading": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "./public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./app.ts"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@dtos": "dist/src/dtos",
    "@entities": "dist/src/entities",
    "@shared": "dist/src/shared",
    "@server": "dist/src/server",
    "@config": "dist/config",
    "@models": "dist/src/models"
  },
  "keywords": [
    "pug",
    "nodejs",
    "sequelizeORM",
    "mysql",
    "mysql2",
    "typescript",
    "nodejs",
    "fullstack"
  ],
  "author": "Than Myat Soe - BroTM",
  "license": "ISC",
  "dependencies": {
    "@types/cookie-parser": "^1.4.4",
    "@types/debug": "^4.1.8",
    "@types/morgan": "^1.9.5",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.1",
    "nodemon": "^3.0.1",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.18",
    "@types/helmet": "^4.0.0",
    "@types/node": "^20.6.2",
    "@types/pug": "^2.0.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
